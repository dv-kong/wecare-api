{
  "compilerOptions": {
    "types": ["node", "jest"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strictPropertyInitialization": false,
    "target": "es6",
    "module": "commonjs",
    "lib": ["es5", "es6"],
    "allowJs": false,
    // "outDir": "./dist",
    "outDir": "./build",
    //"baseUrl": "./",
    "rootDir": "./src",
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}
